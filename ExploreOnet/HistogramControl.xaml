<UserControl x:Class="ExploreOnet.HistogramControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ExploreOnet"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <DockPanel Background="White" HorizontalAlignment="Center" VerticalAlignment="Center" >
            <StackPanel x:Name="axisPanel" Orientation="Horizontal"  DockPanel.Dock="Bottom" />
            <StackPanel x:Name="barPanel" Orientation="Horizontal"  Background="LightGray" />
        </DockPanel>
    </Grid>
</UserControl>
